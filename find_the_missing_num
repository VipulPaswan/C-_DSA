#include<iostream>
using namespace std;

int main(){

          int arr[6] = {1,3,4,5,6};
          int n=6,ans;
          int sum = 0;
          for(int i = 0;i<n-1;i++){

                    sum = sum+arr[i];

                    ans = n*(n+1)/2;
          }
          cout<<ans-sum;




          return 0;
}
